<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Providers List</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <h1>Providers List</h1>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Specialty</th>
            </tr>
        </thead>
        <tbody>
            <%results.forEach(row => {%>
                <tr>
                    <td><%= row.first_name %></td>
                    <td><%= row.last_name %></td>
                    <td><%= row.provider_specialty %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
